#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/dist/bin/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/dist/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/dist/bin/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/dist/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules/next/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/next@15.3.4_@babel+core@7.2_02b70c71677ac7d5c1d8d0208c09f177/node_modules:/proc/cygdrive/c/Users/ATS/ATS_Tech/New folder/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../next/dist/bin/next" "$@"
else
  exec node  "$basedir/../../../../next/dist/bin/next" "$@"
fi
