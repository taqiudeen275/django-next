{
  "name": "@django-next/cli",
  "version": "2.1.0",
  "description": "Generate type-safe Next.js SDKs from Django REST APIs with React Query hooks, Server Actions, and comprehensive TypeScript support",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "bin": {
    "django-next": "dist/src/index.js"
  },
  "keywords": [
    "django",
    "nextjs",
    "typescript",
    "react-query",
    "openapi",
    "rest-api",
    "code-generation",
    "server-actions",
    "zod",
    "validation"
  ],
  "author": "Django-Next Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/django-next/django-next.git",
    "directory": "packages/cli"
  },
  "homepage": "https://github.com/django-next/django-next#readme",
  "bugs": {
    "url": "https://github.com/django-next/django-next/issues"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "format": "prettier --write 'src/**/*.ts' 'src/**/*.tsx'",
    "lint": "eslint 'src/**/*.ts' 'src/**/*.tsx'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "boxen": "^8.0.1",
    "chalk": "^5.4.1",
    "commander": "^11.0.0",
    "openapi-typescript": "^6.5.0",
    "openapi-zod-client": "^1.18.3",
    "zod-openapi": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/jest": "^29.5.0",
    "eslint": "^9.0.0",
    "jest": "^29.5.0",
    "prettier": "^3.2.0",
    "ts-jest": "^29.1.0",
    "typescript": "^5.4.0",
    "ts-node": "^10.9.0"
  }
}
